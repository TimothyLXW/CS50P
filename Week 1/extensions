gif = "image/gif"
jpg = "image/jpeg"
jpeg = "image/jpeg"
png = "image/png"
pdf = "application/pdf"
txt = "text/plain"
zip = "application/zip"

ext = input("File name: ").lower().strip()

if ext.endswith(".gif"):
    print(gif)
elif ext.endswith(".jpg")  or ext.endswith(".jpeg"):
    print(jpg)
elif ext.endswith(".png"):
    print(png)
elif ext.endswith(".pdf"):
    print(pdf)
elif ext.endswith(".txt"):
    print(txt)
elif ext.endswith(".zip"):
    print(zip)
else:
    print("application/octet-stream")
